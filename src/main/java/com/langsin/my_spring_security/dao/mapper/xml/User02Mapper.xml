<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.langsin.my_spring_security.dao.mapper.User02Mapper">


<!--<select id="getUser01" resultType="com.langsin.my_spring_security.pojo.User02">-->
<!--select * from user;-->
<!--</select>-->



<insert id="batshInsert">
insert into user02 values
<foreach collection="list" item="user" index="index" separator="," close=";">
(#{user.userId},#{user.username},#{user.password},#{user.gender},#{user.age},#{user.email},#{user.address},#{user.role})
</foreach>
</insert>

<insert id="addUser" useGeneratedKeys="true" keyProperty="userId" parameterType="com.langsin.my_spring_security.pojo.User02">
insert into user02(username,password,gender,age,email,address) values(#{user.username},#{user.password},#{user.gender},#{user.age},#{user.email},#{user.address});
</insert>

<select id="selectAllPage" resultType="com.langsin.my_spring_security.pojo.User02">
SELECT * from user02 limit #{page},#{size}
</select>


</mapper>